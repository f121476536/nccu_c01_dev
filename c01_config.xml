<?xml version="1.0"?>
<config>
    <to_local>
        <c01_lib>
            <lib_dir>lib</lib_dir>
            <static_dir>api-result</static_dir>
        </c01_lib>
        <usrinfo_js>
            <function_name>GetUserInfo</function_name>
            <file_name>userinfo-result.js</file_name>
        </usrinfo_js>
        <prsresult_js>
            <function_name>GetPRSAPIResult</function_name>
            <file_name>PRS-result.js</file_name>
        </prsresult_js>
        <emotion_result_js>
            <function_name>GetEmotionAPIResult</function_name>
            <file_name>emotion-result.js</file_name>
        </emotion_result_js>
        <audio_anwser_js>
            <function_name>get_audio_anwser</function_name>
            <file_name>audio-anwser-result.js</file_name>
        </audio_anwser_js>
        <audio_text>
            <from_dir>C:\Users\Admin\Downloads</from_dir>
            <to_dir>audio-recognition</to_dir>
        </audio_text>
        <account_cache>
            <file_name>account_cache.txt</file_name>
        </account_cache>
        <c01_camera>
            <save_dir>usr-pic</save_dir>
            <save_done_dir>usr-done-pic</save_done_dir>
        </c01_camera>
        <run_chrome>
            <file_dir>C:\Program Files\Google\Chrome\Application\chrome.exe</file_dir>
            <prs_file>index.html</prs_file>
            <!--用新建使用者模式開啟，並將新建使用者的資料儲存至此目錄-->
            <para_dir_name>chrome-usr</para_dir_name>
            <start_file>startpage.html</start_file>
        </run_chrome>
    </to_local>
    <to_server>
        <alma_api>
            <protocol>https</protocol>
            <address>api-ap.hosted.exlibrisgroup.com</address>
            <name>almaws</name>
            <version>v1</version>
            <key>l8xxc7b903b67fbf4a3db33e7c7d5ff838f6</key>
            <response_format>json</response_format>
        </alma_api>
        <!--NCCU API Server，再取得某讀者的推薦清單-->
        <get_PRS_list>
            <!-- <address>https://sysk8s.lib.nccu.edu.tw</address> -->
            <address>https://sysk8s.lib.nccu.edu.tw</address>
            <port>443</port>
            <version>v1</version>
            <routing>prs_get_topk_list_by_userid</routing>
        </get_PRS_list>
        <!--NCCU API Server情緒分析完後，取得該分析的圖片-->
        <emotion_api>
            <address>https://sysk8s.lib.nccu.edu.tw</address>
            <port>443</port>
            <version>v1</version>
            <routing>face_recognition_emtion</routing>
        </emotion_api>
        <qa_api>
            <address>http://140.119.127.32</address>
            <port>8002</port>
            <version>v1</version>
            <routing>qa_system_question</routing>
        </qa_api>
    </to_server>
    <to_ros>
        <robot_api>
            <protocol>http</protocol>
            <ip>10.234.163.43</ip>
            <port>8882</port>
            <route>GeosatRobot/api</route>
            <option1>Device</option1>
            <option2>Reboot</option2>
            <option3>Shutdown</option3>
        </robot_api>
    </to_ros>
